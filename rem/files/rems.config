config init
	option stdout 1
	option stderr 1
	option user rems
	option group rems
	option respawn 1
#	OS environments pass to rem for config file template, see
#	https://github.com/fatedier/rem#configuration-file-template
#	list env 'ENV_NAME=value'
#	Config files include in temporary config file.
#	list conf_inc '/etc/rem/rems.d/rems_full.ini'

config conf 'common'
	option bind_port 7000
#	List options with name="_" will be directly appended to config file
#	list _ '# Key-A=Value-A'
